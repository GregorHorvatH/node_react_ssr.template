{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie 11"]
        }
      }
    ],
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "react-loadable/babel",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["emotion"]
  ]
}
